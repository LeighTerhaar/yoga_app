<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Sun a #<%= @sun_a.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/sun_as" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link sun_a start -->
      <div class="col">
        <a href="/sun_as/<%= @sun_a.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit sun a
        </a>
      </div>
      <!-- Edit link sun_a end -->

      <!-- Delete link sun_a start -->
      <div class="col">
        <a href="/delete_sun_a/<%= @sun_a.id %>" class="btn btn-block btn-outline-secondary">
          Delete sun a
        </a>
      </div>
      <!-- Delete link sun_a end -->
    </div>

    <dl>
      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@sun_a.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@sun_a.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->
<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "sun_a_templates/sun_a_poses" %>
  </div>
</div>
<!-- Show Page Customization end -->
