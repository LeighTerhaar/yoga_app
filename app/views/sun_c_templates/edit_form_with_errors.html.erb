<!-- Validation failure messages -->
<% if @sun_c.errors.any? %>
  <% @sun_c.errors.full_messages.each do |message| %>
    <div class="alert">
      <%= message %>
    </div>
  <% end %>
<% end %>

<h1>
  Edit sun c #<%= @sun_c.id %>
</h1>

<hr>

<!-- Form sun_c start -->
<form action="/update_sun_c/<%= @sun_c.id %>" method="post">
<!-- Form sun_c end -->

  <button class="btn btn-block btn-outline-secondary">
    Update sun c
  </button>
</form>

<hr>

<a href="/sun_cs">
  Go back
</a>
