<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All sun cs
    </h1>

    <a href="/sun_cs/new" class="btn btn-block btn-outline-secondary">
      Add a new sun c
    </a>
  </div>
</div>

<div class="row">
  <div class="col-md-12 mb-3">
    <%= render partial: "sun_c_templates/sun_cs_search_form" %>
  </div>
</div>



<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @sun_cs.each do |sun_c| %>
      <tr>
        <td>
          <%= sun_c.id %>
        </td>

        <td>
          <%= time_ago_in_words(sun_c.created_at) %> ago
        </td>

        <td>
          <%= time_ago_in_words(sun_c.updated_at) %> ago
        </td>

        <td>
          <a href="/sun_cs/<%= sun_c.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @sun_cs, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
